con
        lr = $1f6
        r0 = 23
        r1 = 22
        r2 = 21

dat
        orgh    $400
        .text
        .global _cognew2
_cognew2
       mov     r2, #$10
       setq    r1
       coginit r2, r0 wc
 if_nc mov     r0, r2
 if_c  neg     r0, #1
       jmp     lr

        .global _coginit2
_coginit2
        setq    r2
        coginit r0, r1
        jmp     lr

        .global _cogstop2
_cogstop2
        cogstop r0
        jmp     lr
